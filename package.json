{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "vite preview",
    "lint": "npx eslint src/ --ext .tsx",
    "lint:fix": "npx eslint src/ --ext .tsx --fix",
    "vi:test": "vitest --ui",
    "vi:coverage": "vitest run --coverage",
    "cypress:open": "cypress open --e2e",
    "cy:run-e2e": "cypress run --e2e",
    "coverage": "cypress run --e2e && npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@testing-library/user-event": "^14.4.3",
    "@vitest/ui": "^0.29.3",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "cross-fetch": "^3.1.5",
    "express": "^4.18.2",
    "immer": "^9.0.21",
    "istanbul": "^0.4.5",
    "module-alias": "^2.2.2",
    "moxios": "^0.4.0",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-svg": "^16.1.6",
    "ts-node": "^10.9.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-plugin-svgr": "^2.4.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/moxios": "^0.4.15",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^12.10.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "jsdom": "^21.1.1",
    "lint-staged": "^12.4.1",
    "msw": "^1.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "sass": "^1.59.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.29.2"
  },
  "_moduleAliases": {
    "@modules": "dist/rest/modules",
    "@services": "dist/services"
  }
}
